{
  "name": "tacowasa",
  "version": "0.0.1",
  "description": "Web based digital Kanban tool",
  "main": "app.js",
  "scripts": {
    "start": "node ./bin/www",
    "build": "webpack",
    "watch": "webpack --watch",
    "lint": "eslint",
    "lint:all": "eslint . --ext .js",
    "migrate": "sequelize db:migrate && sequelize db:migrate --migrations-path addons/github/migrations && sequelize db:migrate --migrations-path addons/stats/migrations",
    "migrate:test": "cross-env NODE_ENV=test sequelize db:migrate && cross-env NODE_ENV=test sequelize db:migrate --migrations-path addons/github/migrations && cross-env NODE_ENV=test sequelize db:migrate --migrations-path addons/stats/migrations",
    "test": "cross-env NODE_ENV=test mocha --reporter spec",
    "test:all": "cross-env NODE_ENV=test mocha --opts mocha.all.opts",
    "test:cover": "cross-env NODE_ENV=test nyc npm run test:all",
    "coveralls": "cross-env NODE_ENV=test cat ./coverage/lcov.info | coveralls",
    "component:create": "bash ./scripts/create_component.sh"
  },
  "nyc": {
    "lines": 0,
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "exclude": [
      "**/spec/**",
      "**/migrations/**",
      "public/dist/**",
      "node_modules/"
    ],
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "all": true,
    "check-coverage": true,
    "sourceMap": false,
    "instrument": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kait-tt/tacowasa.git"
  },
  "keywords": [
    "web",
    "kanban"
  ],
  "author": "snakazawa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kait-tt/tacowasa/issues"
  },
  "homepage": "https://github.com/Kait-tt/tacowasa#readme",
  "engines": {
    "node": "~6.3.1"
  },
  "dependencies": {
    "babel": "^6.5.2",
    "babel-loader": "^6.3.2",
    "babel-plugin-istanbul": "^2.0.1",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-register": "^6.16.3",
    "block-ui": "^2.70.1",
    "bluebird": "^3.4.1",
    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.7",
    "bootstrap-sass": "^3.3.7",
    "bootstrap-select": "^1.11.0",
    "chai-as-promised": "^5.3.0",
    "co": "^4.6.0",
    "commander": "^2.9.0",
    "config": "^1.21.0",
    "connect-mysql": "^2.1.1",
    "cookie-parser": "^1.4.3",
    "cross-env": "^2.0.0",
    "css-loader": "^0.24.0",
    "debug": "^2.2.0",
    "ejs": "^2.5.1",
    "eventemitter2": "^2.1.0",
    "express": "^4.14.0",
    "express-session": "^1.14.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.9.0",
    "github": "^3.1.0",
    "highcharts": "^5.0.4",
    "html-loader": "^0.4.4",
    "jquery": "^3.1.0",
    "jquery-ui": "^1.12.0",
    "jquery.easing": "^1.4.1",
    "knockout": "^3.4.0",
    "lodash": "^4.14.2",
    "marked": "^0.3.6",
    "moment": "^2.14.1",
    "morgan": "^1.7.0",
    "mysql": "^2.11.1",
    "node-sass": "^3.8.0",
    "nyc": "^8.3.0",
    "passport": "^0.3.2",
    "passport-github": "^1.1.0",
    "passport-stub": "^1.1.1",
    "request": "^2.75.0",
    "request-promise": "^4.1.1",
    "sass-loader": "^4.1.1",
    "sequelize": "^3.23.6",
    "sequelize-cli": "^2.4.0",
    "serve-favicon": "^2.3.0",
    "socket.io": "^1.4.8",
    "socket.io-client": "^1.4.8",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.12",
    "electron": "^1.4.2",
    "eslint": "^3.10.2",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-standard": "^2.0.1",
    "istanbul": "^0.4.4",
    "mocha": "^3.0.2",
    "mocha-lcov-reporter": "^1.2.0",
    "nightmare": "^2.7.0",
    "sinon": "^1.17.6",
    "supertest": "^2.0.0"
  }
}

{
  "name": "tacowasa",
  "version": "0.0.1",
  "description": "Web based digital Kanban tool",
  "main": "app.js",
  "scripts": {
    "start": "node ./bin/www",
    "build": "webpack",
    "watch": "webpack --watch",
    "migrate": "sequelize db:migrate",
    "migrate:test": "cross-env NODE_ENV=test sequelize db:migrate",
    "test": "cross-env NODE_ENV=test mocha --reporter spec --timeout 10000",
    "test:all": "cross-env NODE_ENV=test npm run test --recursive spec/schemes/ spec/models/ spec/routes/ addons/github/spec/schemas",
    "test:cover": "cross-env NODE_ENV=test istanbul cover node_modules/mocha/bin/_mocha -- -u exports --timeout 10000 --recursive spec/schemes/ spec/models/ spec/routes/ addons/github/spec/schemas",
    "coveralls": "cross-env NODE_ENV=test cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kait-tt/tacowasa.git"
  },
  "keywords": [
    "web",
    "kanban"
  ],
  "author": "snakazawa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kait-tt/tacowasa/issues"
  },
  "homepage": "https://github.com/Kait-tt/tacowasa#readme",
  "engines": {
    "node": "~6.3.1"
  },
  "dependencies": {
    "babel": "^6.5.2",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "block-ui": "^2.70.1",
    "bluebird": "^3.4.1",
    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.7",
    "bootstrap-sass": "^3.3.7",
    "bootstrap-select": "^1.11.0",
    "co": "^4.6.0",
    "config": "^1.21.0",
    "connect-mysql": "^2.1.1",
    "cookie-parser": "^1.4.3",
    "cross-env": "^2.0.0",
    "css-loader": "^0.24.0",
    "debug": "^2.2.0",
    "ejs": "^2.5.1",
    "eventemitter2": "^2.1.0",
    "express": "^4.14.0",
    "express-session": "^1.14.0",
    "file-loader": "^0.9.0",
    "github": "^3.1.0",
    "html-loader": "^0.4.3",
    "jquery": "^3.1.0",
    "jquery-ui": "^1.12.0",
    "jquery.easing": "^1.4.1",
    "knockout": "^3.4.0",
    "lodash": "^4.14.2",
    "marked": "^0.3.6",
    "moment": "^2.14.1",
    "morgan": "^1.7.0",
    "mysql": "^2.11.1",
    "node-sass": "^3.8.0",
    "passport": "^0.3.2",
    "passport-github": "^1.1.0",
    "sass-loader": "^4.0.0",
    "sequelize": "^3.23.6",
    "sequelize-cli": "^2.4.0",
    "serve-favicon": "^2.3.0",
    "socket.io": "^1.4.8",
    "socket.io-client": "^1.4.8",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "latest"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.12",
    "istanbul": "^0.4.4",
    "mocha": "^3.0.2",
    "mocha-lcov-reporter": "^1.2.0",
    "supertest": "^2.0.0"
  }
}
